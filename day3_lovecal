# ðŸš¨ Don't change the code below ðŸ‘‡
print("Welcome to the Love Calculator!")
name1 = input("What is your name? \n")
name2 = input("What is their name? \n")
# ðŸš¨ Don't change the code above ðŸ‘†

#Write your code below this line ðŸ‘‡

lower_name1 = name1.lower()
lower_name2 = name2.lower()
sum_name = lower_name1 + lower_name2
#print(sum_name)
count_t = sum_name.count("t")
count_r = sum_name.count("r")
count_u = sum_name.count("u")
count_e = sum_name.count("e")
count_true = int(count_t) + int(count_r) + int(count_u) + int(count_e)
#print(count_true)

count_l = sum_name.count("l")
count_o = sum_name.count("o")
count_v = sum_name.count("v")
count_e2 = sum_name.count("e")
count_love = int(count_l) + int(count_o) + int(count_v) + int(count_e2)
#print(count_love)

love_score = str(count_true) + str(count_love)
#print(love_score)
love_score_int = int(love_score)
#print(f"Your score is {love_score})

if love_score_int < 10 or love_score_int > 90:
    print(f"Your score is {love_score_int}, you go together like coke and mentos.")

elif 40 < love_score_int < 50:
    print(f"Your score is {love_score_int}, you are alright together.")

else:
    print(f"Your score is {love_score_int}.")
